@ARTICLE{Josephs2017-kp,
  title    = "Rates of hippocampal atrophy and presence of post-mortem {TDP-43}
              in patients with Alzheimer's disease: a longitudinal
              retrospective study",
  author   = "Josephs, Keith A and Dickson, Dennis W and Tosakulwong, Nirubol
